___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const injectScript = require("injectScript");
const encodeUriComponent = require("encodeUriComponent");
const queryPermission = require("queryPermission");
const setDefaultConsentState = require("setDefaultConsentState");
const JSON = require("JSON");

const cookiebotSerial = data.serial;
const IABEnabled = data.iabFramework;
const language = data.language;

let scriptUrl =
  "https://consent.cookiebot.com/uc.js?cbid=" +
  encodeUriComponent(cookiebotSerial);

if (language === "variable") {
  scriptUrl += "&culture=" + encodeUriComponent(data.languageVariable);
}

const georegionsTable = data.georegionsTable;
let georegions = [];
if (georegionsTable) {
  for (let index = 0; index < georegionsTable.length; index++) {
    const georegion = georegionsTable[index];
    switch (georegion.regulations) {
      case "ccpa":
        georegions.push({
          region: "us-06",
          cbid: georegion.cbid,
        });
        break;
      case "br":
        georegions.push({
          region: "br",
          cbid: georegion.cbid,
        });
        break;
      case "gdpr":
        georegions.push({
          region:
            "at,be,bg,ch,cy,de,dk,ee,es,fi,fr,gb,gg,gr,hr,hu,ie,im,it,je,lt,lu,lv,mt,nl,pl,pt,ro,se,si,sk",
          cbid: georegion.cbid,
        });
        break;
      default:
        break;
    }
  }
  scriptUrl += "&georegions=" + encodeUriComponent(JSON.stringify(georegions));
}

if (IABEnabled) {
  scriptUrl += "&framework=IAB";
}

setDefaultConsentState({
  functionality_storage: data.defaultConsentPreferences,
  personalization_storage: data.defaultConsentPreferences,
  analytics_storage: data.defaultConsentStatistics,
  ad_storage: data.defaultConsentMarketing,
  security_storage: "granted",
  wait_for_update: data.waitForUpdate,
});

if (queryPermission("inject_script", scriptUrl)) {
  injectScript(scriptUrl, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  data.gtmOnFailure();
}


